{
  "name": "@zaob/glean-debug-logger",
  "version": "1.0.0",
  "description": "React/Next.js debug logging library that captures console logs, network requests, exports with smart filenames, and supports server upload",
  "author": "Zaob <zaob.ogn@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maemreyo/glean-debug-logger.git"
  },
  "bugs": {
    "url": "https://github.com/maemreyo/glean-debug-logger/issues"
  },
  "homepage": "https://github.com/maemreyo/glean-debug-logger#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "semantic-release": "^25.0.2",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src"
  },
  "keywords": [
    "react",
    "debug",
    "logging",
    "console",
    "network",
    "fetch",
    "xhr",
    "debugger"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
