{
  "name": "@zaob/glean-debug-logger",
  "version": "3.1.0",
  "description": "React/Next.js debug logging library that captures console logs, network requests, exports with smart filenames, and supports server upload",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "author": "Zaob <zaob.ogn@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maemreyo/glean-debug-logger.git"
  },
  "bugs": {
    "url": "https://github.com/maemreyo/glean-debug-logger/issues"
  },
  "homepage": "https://github.com/maemreyo/glean-debug-logger#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/csf": "^0.1.13",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.1.0",
    "semantic-release": "^25.0.2",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --config-dir .storybook"
  },
  "keywords": [
    "react",
    "debug",
    "logging",
    "console",
    "network",
    "fetch",
    "xhr",
    "debugger"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "framer-motion": "^12.28.1",
    "goober": "^2.1.18",
    "lucide-react": "^0.562.0"
  }
}
